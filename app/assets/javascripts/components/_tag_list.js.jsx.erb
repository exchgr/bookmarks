var TagList = React.createClass({
  render: function() {
    var tagNodes = this.props.tags.map(function(tag) {
      return(
        <li key={tag.id}>
          <a href={"/tags/" + tag.name}>{tag.name}</a>
        </li>
      )
    });

    return (
      <ul className="tags">
        {tagNodes}
      </ul>
    );
  }
});
