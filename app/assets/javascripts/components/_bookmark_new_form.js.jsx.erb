var BookmarkNewForm = React.createClass({
  renderedForm: function() {
    return $(".rendered-bookmark-new-form");
  },

  render: function() {
    return (
      <BookmarkForm
        headerText="Add a bookmark"
        displaying={this.props.displaying}
        onSubmit={this.props.onSubmit}
        errors={this.props.errors}
        renderedForm={this.renderedForm}
        toggleDisplaying={this.props.toggleDisplaying}
        className="rendered-bookmark-new-form"
      />
    );
  }
});
